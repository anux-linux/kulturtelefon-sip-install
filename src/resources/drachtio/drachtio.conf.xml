<drachtio>
  <sip>
    <contacts>
      <contact dns-names="DRACHTIO_DNS_PLACEHOLDER" external-ip="DRACHTIO_EXTERNAL_IP_PLACEHOLDER">sip:DRACHTIO_LOCAL_IP_PLACEHOLDER;transport=udp,tcp</contact>
    </contacts>
  </sip>

  <admin>
    <ip>127.0.0.1</ip>
    <port>9022</port>
    <secret>DRACHTIO_SECRET_PLACEHOLDER</secret>
  </admin>

  <logging>
    <loglevel>info</loglevel>
    <file>
      <name>/var/log/drachtio/drachtio.log</name>
      <archive>/var/log/drachtio/archive</archive>
      <size>50</size>
      <maxSize>100</maxSize>
      <minSize>2000</minSize>
      <auto-flush>true</auto-flush>
    </file>
  </logging>
</drachtio>